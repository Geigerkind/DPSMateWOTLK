	<Button name="DPSMate_Details_CureDisease_ScrollButton" virtual="true">
		<Size x="220" y="24" />
		<Layers>
			<Layer level="ARTWORT">
				<Texture name="$parent_Icon" file="Interface\AddOns\DPSMate\images\dummy">
					<Size x="24" y="24" />
					<Anchors>
						<Anchor point="TOPLEFT" />
					</Anchors>
				</Texture>
				<FontString name="$parent_Name" inherits="GameFontNormalSmall" text="Killed by Firy Elemental of Molten Core" justifyH="LEFT">
					<Size x="130" y="24" />
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parent_Icon" relativePoint="RIGHT">
							<Offset x="5" y="0" />
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parent_Value" inherits="GameFontNormalSmall" text="12:25 PM">
					<Size x="70" y="24" />
					<Anchors>
						<Anchor point="RIGHT">
							<Offset x="-5" y="0" />
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="BACKGROUND">
				<Texture name="$parent_selected">
					<Anchors>
						<Anchor point="TOPLEFT" />
						<Anchor point="BOTTOMRIGHT" />
					</Anchors>
					<Color r="1" g="1" b="1" a="0.5" />
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnClick>
				if this:GetParent():GetParent():GetName() == "DPSMate_Details_CompareCureDisease" then
					if this:GetParent():GetName()=="DPSMate_Details_CompareCureDisease_Log" then
						DPSMate.Modules.DetailsCureDisease:SelectCreatureButton(this:GetID(),"Compare")
					elseif this:GetParent():GetName()=="DPSMate_Details_CompareCureDisease_LogTwo" then
						DPSMate.Modules.DetailsCureDisease:SelectCreatureAbilityButton(DPSMate_Details_CompareCureDisease_LogTwo_ScrollFrame.index, this:GetID(),"Compare")
					end
				else
					if this:GetParent():GetName()=="DPSMate_Details_CureDisease_Log" then
						DPSMate.Modules.DetailsCureDisease:SelectCreatureButton(this:GetID(),"")
					elseif this:GetParent():GetName()=="DPSMate_Details_CureDisease_LogTwo" then
						DPSMate.Modules.DetailsCureDisease:SelectCreatureAbilityButton(DPSMate_Details_CureDisease_LogTwo_ScrollFrame.index, this:GetID(),"")
					end
				end
			</OnClick>
		</Scripts>
	</Button>
	
